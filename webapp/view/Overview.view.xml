<mvc:View
    controllerName="logaligroup.sapui5.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    displayBlock="true">
    <Page title="{i18n>appTitle}">
    <landmarkInfo>
        <PageAccessibleLandmarkInfo rootRole="Region" rootLabel="{i18n>OvRootLabel}"
                                    contentRole="Main" contentLabel="OvContentLabel"
                                    headerRole="Banner" headerLabel="{i18n>OvHeaderLabel}"/>
    </landmarkInfo>
        <headerContent>
            <Button
                icon="sap-icon://action"
                press=".onOpenDialogHeader"
                class="sapUiSmallMarginEnd"
            />
        </headerContent>
        <content>
            <mvc:XMLView viewName="logaligroup.sapui5.view.HelloPanel" />
            <mvc:XMLView viewName="logaligroup.sapui5.view.InvoicesList" />
        </content>
    </Page>
</mvc:View>
